@model IEnumerable<PlayerManagement.Models.PlayerAchievement>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.achievementName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.number)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.achievementName) 
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.number)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.playerID }) |
            @Html.ActionLink("Details", "Details", new { id=item.playerID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.playerID })
        </td>
    </tr>
}

</table>
